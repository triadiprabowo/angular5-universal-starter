{
  "name": "angular5-universal-starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Triadi Prabowo",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "optimize": "./node_modules/.bin/gulp",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --deploy-url=/dist && ng build --prod --app 1 --output-hashing=false",
    "build:prerender": "npm run build:prod && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:prod && npm run webpack:server && npm run optimize",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc.2",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express-urlrewrite": "^1.2.0",
    "hammerjs": "^2.0.8",
    "js-cookie": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "moment": "^2.20.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "rxjs": "^5.5.2",
    "underscore": "^1.8.3",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/node": "^8.0.30",
    "cpy-cli": "^1.0.1",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-color": "0.0.1",
    "gulp-gzip": "^1.4.1",
    "gulp-imagemin": "^4.1.0",
    "http-server": "^0.10.0",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.7",
    "typescript": "~2.4.2"
  }
}
